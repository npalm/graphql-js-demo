{
  "name": "next-build-demo",
  "description": "Nextbuild demo GraphQL Server based on Apollo.",
  "scripts": {
    "start": "nodemon ./server.js --exec babel-node",
    "lint": "eslint ."
  },
  "keywords": [
    "Javascript",
    "GraphQL"
  ],
  "author": "Niek Palm",
  "license": "MIT",
  "dependencies": {
    "apollo-server": "^0.2.8",
    "body-parser": "^1.15.2",
    "express": "4.13.4",
    "graphql": "^0.7.0",
    "graphql-tools": "^0.7.2",
    "lodash": "^4.17.4",
    "mongoose": "^4.9.8",
    "request-promise": "^3.0.0",
    "sequelize": "^3.30.4",
    "sqlite": "^2.7.0",
    "sqlite3": "^3.1.8"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-polyfill": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "casual": "^1.5.2",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-import": "^1.1.0",
    "eslint-plugin-react": "^4.2.3",
    "graphql": "^0.5.0",
    "nodemon": "^1.9.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  }
}
